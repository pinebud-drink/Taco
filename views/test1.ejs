        <main>
            <form>
                name : <input type="text" name="name">
                phone : <input type="text" name="phone"><br>
            </form>
            <button class="send">send</button><br>

            <div class="name_data"></div>
            <div class="phone_data"></div>

            <script>
                document.querySelector('.send').addEventListener('click',function(){
                    var name = document.forms[0].elements[0].value;
                    var phone = document.forms[0].elements[1].value;
                    sendFunction('localhost:8000/test1',name,phone)
                })
                function sendFunction(url,data1,data2){
                    var data = {'name':data1,'phone':data2}
                    data = JSON.stringify(data)

                    var xhRequest = new XMLHttpRequest()
                    xhRequest.open('POST',url)
                    xhRequest.setRequestHeader('Content-Type',"application/json")
                    xhRequest.send(data)
                    xhRequest.addEventListener('load',function(){
                        var result = xhRequest.responseText;
                        result = JSON.parse(result)
                        document.querySelector(".name_data").innerHTML = result.name;
                        document.querySelector(".phone_data").innerHTML = result.phone;

                    })
                }
            </script>
